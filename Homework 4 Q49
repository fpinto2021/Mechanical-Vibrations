          "output_type": "display_data",
          "data": {
            "text/plain": [
              "<Figure size 800x500 with 1 Axes>"
            ],
            "image/png": "\n"
          },
          "metadata": {}
        }
      ],
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "import matplotlib.cm as cm\n",
        "\n",
        "# Given Parameters\n",
        "m = 4\n",
        "v0 = 0.85\n",
        "\n",
        "# Problem Options\n",
        "options = {\n",
        "    \"A\": {\"c\": 40, \"k\": 100},\n",
        "    \"B\": {\"c\": 55, \"k\": 170},\n",
        "    \"C\": {\"c\": 60, \"k\": 240}\n",
        "}\n",
        "\n",
        "# Time\n",
        "t = np.linspace(0, 5, 500)\n",
        "\n",
        "# Create the Figure\n",
        "fig, ax = plt.subplots(figsize=(8, 5))\n",
        "\n",
        "# Just for Rainbow Lines\n",
        "colors = cm.rainbow(np.linspace(0, 1, len(t)))\n",
        "\n",
        "for option, params in options.items():\n",
        "    c = params[\"c\"]\n",
        "    k = params[\"k\"]\n",
        "\n",
        "    # Computing Natural Frequency\n",
        "    omega_n = np.sqrt(k / m)\n",
        "\n",
        "    # Computing Damping Ratio\n",
        "    zeta = c / (2 * m * omega_n)\n",
        "\n",
        "    # Check for Underdamped\n",
        "    if zeta < 1:\n",
        "        # Computing Damped\n",
        "        omega_d = omega_n * np.sqrt(1 - zeta**2)\n",
        "\n",
        "        # Computing Response\n",
        "        x_t = (v0 / omega_d) * np.exp(-zeta * omega_n * t) * np.sin(omega_d * t)\n",
        "\n",
        "        # Plotting the response\n",
        "        for i in range(len(t) - 1):\n",
        "            ax.plot(t[i:i+2], x_t[i:i+2], color=colors[i], linewidth=2)\n",
        "\n",
        "        # Legend for the Plot\n",
        "        ax.plot([], [], color='black', label=f\"Option {option} (ζ = {zeta:.3f})\")\n",
        "\n",
        "ax.set_xlabel(\"Time (s)\")\n",
        "ax.set_ylabel(\"Displacement x(t) (m)\")\n",
        "ax.set_title(\"System Response with Rainbow Effect\")\n",
        "ax.legend()\n",
        "ax.grid()\n",
        "\n",
        "plt.show()\n"
      ]
    }
  ]
}